{
  "extends": ["tslint:recommended", "tslint-sonarts"],
  "rules": {
    "interface-name" : [true, "never-prefix"],
    "object-literal-sort-keys": false
  },
  "globals": {
      "globals": false,
      "Intl": false,
      "CHECK": false,
      "CHECK_EQ": false,
      "CHECK_GE": false,
      "CHECK_GT": false,
      "CHECK_LE": false,
      "CHECK_LT": false,
      "CHECK_NE": false,
      "DCHECK": false,
      "DCHECK_EQ": false,
      "DCHECK_GE": false,
      "DCHECK_GT": false,
      "DCHECK_LE": false,
      "DCHECK_LT": false,
      "DCHECK_NE": false,
      "global": false,
      "require": false,
      "process": false,
      "exports": false,
      "module": false,
      "internalBinding": false,
      "primordials": true
  }
}
